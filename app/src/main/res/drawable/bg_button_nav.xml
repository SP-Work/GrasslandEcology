<?xml version="1.0" encoding="UTF-8"?>
<inset
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:insetLeft="0.0px"
    android:insetRight="0.0px" >

    <selector>
        <item android:state_pressed="true">
            <shape>
                <solid
                    android:color="#a0e0b071" />

                <corners
                    android:radius="50dp" />

                <stroke
                    android:width="1dp"
                    android:color="#a0a67637" />
            </shape>
        </item>
        <item>
            <shape>
                <solid
                    android:color="#02c488" />

                <corners
                    android:radius="50dp" />

                <stroke
                    android:width="1dp"
                    android:color="#04a070" />
            </shape>
        </item>
    </selector>
</inset>